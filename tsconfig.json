{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "types": ["react", "react-native"],
    "noEmit": true,
    "strict": true
  }
}
