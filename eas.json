{
  "cli": {
    "version": ">= 16.17.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "BUILD_PROFILE": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "BUILD_PROFILE": "preview"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "BUILD_PROFILE": "production"
      }
    }
  }
}
